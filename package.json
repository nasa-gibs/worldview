{
  "name": "worldview",
  "version": "1.10.0",
  "description": "Interactive interface for browsing full-resolution, global, near real-time satellite imagery",
  "homepage": "https://worldview.earthdata.nasa.gov/",
  "bugs": {
    "url": "https://github.com/nasa-gibs/worldview/issues",
    "email": "support@earthdata.nasa.gov"
  },
  "license": "NASA-1.3",
  "repository": "nasa-gibs/worldview",
  "engines": {
    "node": "8.8.1"
  },
  "scripts": {
    "preinstall": "node version-check.js",
    "start": "node ./bin/www",
    "test": "grunt test lint",
    "e2e": "run-s --print-name --continue-on-error --silent e2e:*",
    "e2e:chrome": "nightwatch -e chrome",
    "e2e:firefox": "nightwatch -e firefox --skiptags wip",
    "browserstack": "node e2e/browserstack.js -c e2e/browserstack.conf.js -e browserstack --skiptags wip",
    "report": "node e2e/create-report.js",
    "build-js": "node ./tasks/build-js && grunt"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "jquery-ui": "./web/ext/ui/jquery-ui-1.11.4/jquery-ui.js"
  },
  "browserify-shim": {
    "jquery": "$",
    "jquery-ui": { "exports": null, "depends": [ "jquery:$" ] }
  },
  "devDependencies": {
    "autoprefixer": "7.1.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "browserify-shim": "^3.8.14",
    "browserstack-capabilities": "^0.4.0",
    "browserstack-local": "^1.3.0",
    "buster": "0.7.12",
    "chromedriver": "2.25.1",
    "cucumber": "^3.0.2",
    "cucumber-html-reporter": "^3.0.4",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "4.15.2",
    "geckodriver": "1.2.0",
    "grunt": "0.4.x",
    "grunt-buster": "0.4.x",
    "grunt-contrib-clean": "1.0.x",
    "grunt-contrib-compress": "0.5.x",
    "grunt-contrib-concat": "1.0.x",
    "grunt-contrib-copy": "1.0.x",
    "grunt-contrib-cssmin": "1.0.x",
    "grunt-contrib-uglify": "2.0.x",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-exec": "0.4.x",
    "grunt-git-rev-parse": "0.1.x",
    "grunt-line-remover": "0.0.x",
    "grunt-markdown": "0.7.x",
    "grunt-minjson": "0.4.x",
    "grunt-mkdir": "0.1.x",
    "grunt-postcss": "^0.8.0",
    "grunt-rename": "0.1.x",
    "grunt-stylefmt": "^3.0.0",
    "grunt-text-replace": "0.4.x",
    "gruntify-eslint": "^4.0.0",
    "moment": "2.13.x",
    "nightwatch": "^0.9.16",
    "nightwatch-cucumber": "^8.1.0",
    "npm-run-all": "^4.1.1",
    "phantomjs": "2.1.x",
    "selenium-server-standalone-jar": "3.0.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.0.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "babel-polyfill": "6.16.0",
    "bluebird": "3.4.6",
    "clipboard": "^1.7.1",
    "font-awesome": "4.7.0",
    "gifshot": "https://github.com/nasa-gibs/gifshot/archive/4.2.0.tar.gz",
    "icheck": "^1.0.2",
    "isotope-layout": "3.0.x",
    "isotope-packery": "2.x.x",
