    "snyk": "^1.316.1"
  },
  "browser": {
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "browserslist": [
    "last 5 versions",
    "not ie < 11",
    "not edge < 15",
    "> 2%"
  ],
  "jest": {
    "setupFiles": [
      "./test-env.js"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "moduleNameMapper": {
      "^googleTagManager$": "<rootDir>/web/js/components/util/google-tag-manager.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(ol|ol-mapbox-style|@mapbox)/)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build/"
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "snyk": true
}
