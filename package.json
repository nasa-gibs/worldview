    "perfect-scrollbar": "^1.2.0",
    "proj4": "2.3.3",
    "promise-queue": "2.2.3",
    "react": "15.3.1",
    "react-dom": "15.3.1",
    "worldview-components": "^1.5.2",
    "worldview-options-eosdis": "github:nasa-gibs/worldview-options-eosdis#v2.1.2"
  }
}
