{
  "name": "worldview",
  "version": "2.12.0",
  "description": "Interactive interface for browsing full-resolution, global satellite imagery",
  "keywords": [
    "NASA",
    "Earthdata",
    "GIS",
    "NRT",
    "satellite imagery",
    "wildfire management",
    "air quality",
    "flood monitoring",
    "science",
    "earth science",
    "GDAL"
  ],
  "homepage": "https://worldview.earthdata.nasa.gov/",
  "bugs": {
    "url": "https://github.com/nasa-gibs/worldview/issues",
    "email": "support@earthdata.nasa.gov"
  },
  "license": "NASA-1.3",
  "repository": "nasa-gibs/worldview",
  "engines": {
    "node": "8.8.1"
  },
  "scripts": {
    "analyze": "cross-env ANALYZE_MODE=true NODE_ENV=production webpack",
    "browserstack": "node e2e/browserstack.js -c e2e/browserstack.conf.js -e browserstack ",
    "build": "run-s clean getcapabilities build:config build:dev",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "build:config": "bash -c 'tasks/buildOptions.sh' && node ./tasks/config.js",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "clean": "node ./tasks/clean.js",
    "dist": "npm run clean && npm run getcapabilities && npm run build:config && npm run build:prod && node ./tasks/dist.js",
    "dist:dev": "npm run clean && npm run getcapabilities && npm run build:config && npm run build:dev && node ./tasks/dist.js",
    "distclean": "npm run clean && node ./tasks/distclean.js",
    "docker:ci": "npm run docker:restart && docker exec -it worldview /bin/bash /build/e2e/docker-run.sh ; npm run docker:stop",
    "docker:e2e": "npm run docker:restart && docker exec -it worldview xvfb-run -s '-screen 0, 1920x1080x24' npx nightwatch --env firefox",
    "docker:image": "docker build -t worldview .",
    "docker:reimage": "docker rmi -f worldview && docker build --no-cache -t worldview .",
    "docker:restart": "npm run docker:stop && npm run docker:start",
    "docker:shell": "docker exec -it worldview /bin/bash ; true",
    "docker:start": "docker run --rm --name worldview --mount type=bind,source=\"$(pwd)\",target=/build --mount type=volume,target=/build/node_modules -p 3128:3000 -d worldview",
    "docker:stop": "docker stop -t 1 worldview ; true",
    "e2e": "run-s --continue-on-error --silent e2e:*",
    "e2e:chrome": "npx nightwatch -e chrome",
    "e2e:firefox": "npx nightwatch -e firefox --skiptags wip",
    "getcapabilities": "bash -c 'FETCH_GC=1 tasks/buildOptions.sh'",
    "linkcheck": "node ./tasks/linkcheck",
    "lint": "run-s lint:*",
    "lint:css": "stylelint ./web/css/**/*.css",
    "lint:js": "eslint .",
    "preinstall": "node tasks/checkNodeVersion.js",
    "postinstall": "npm run python",
    "python": "run-script-os",
    "python:darwin:linux": "bash -c 'tasks/pythonInstall.sh linux' ",
    "python:win32": "bash -c 'tasks/pythonInstall.sh'",
    "report": "node e2e/create-report.js",
    "start": "node ./tasks/start.js",
    "test": "npm run test:unit && npm run test:unit:tz && npm run lint",
    "test:unit": "npx jest",
    "test:unit:tz": "TZ=Australia/Sydney npx jest",
    "updateconfig": "run-s getcapabilities build:config",
    "watch": "cross-env NODE_ENV=development webpack-dev-server",
    "upload": "node tasks/upload.js"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^7.3.0",
    "browserstack-capabilities": "^0.4.0",
    "browserstack-local": "^1.3.0",
    "buster": "github:busterjs/buster#v0.8.0",
    "canvas": "^1.6.11",
    "cheerio": "^1.0.0-rc.2",
    "chromedriver": "2.25.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.1",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "eslint": "^4.13.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.22.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^3.0.1",
    "expose-loader": "^0.7.5",
    "express": "4.15.2",
    "file-loader": "^1.1.11",
    "geckodriver": "^1.12.0",
    "glob": "^7.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "jest": "^23.5.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "moment": "^2.19.3",
    "nightwatch": "^0.9.16",
    "node-fetch": "^2.1.2",
    "node-sass": "^4.9.2",
    "node-ssh": "^5.1.1",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "postcss-cli": "^4.1.1",
    "postcss-import": "^11.0.0",
    "postcss-less": "^3.1.0",
    "postcss-loader": "^2.1.6",
    "postcss-preset-env": "^6.4.0",
    "postcss-url": "^7.2.1",
    "precss": "^3.1.2",
    "rc-slider": "8.6.3",
    "react-test-renderer": "^16.5.2",
    "run-script-os": "^1.0.3",
    "sass-loader": "^7.1.0",
    "selenium-server-standalone-jar": "^3.12.0",
    "shelljs": "^0.8.2",
    "showdown": "^1.8.6",
    "style-loader": "^0.21.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.0.0",
    "sugarss": "^2.0.0",
    "tar": "^4.4.4",
    "uglify-js": "^3.4.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "underscore": "^1.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "write-file-webpack-plugin": "^4.3.2",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.5.0",
    "bluebird": "3.4.6",
    "bootstrap": "^4.1.0",
    "cachai": "^1.0.2",
    "canvg-browser": "^1.0.0",
    "clipboard": "^1.7.1",
    "date-fns": "^1.29.0",
    "file-saver": "^1.3.8",
    "icheck": "^1.0.2",
    "immutability-helper": "^2.9.0",
