  },
  "browser": {
    "d3": "./web/ext/d3/d3.js",
    "gifwriter": "./web/ext/gifwriter/gifwriter.js",
    "neuquant": "./web/ext/neuquant/neuquant.js",
    "mobiscroll": "./web/ext/mobiscroll-2.6.0/mobiscroll.js",
    "isotope-layout": "./node_modules/isotope-layout/dist/isotope.pkgd.js",
    "isotope": "./node_modules/isotope-layout/dist/isotope.pkgd.js",
    "jquery-jcrop": "./node_modules/jquery-jcrop/js/jquery.Jcrop.js",
    "jquery": "./node_modules/jquery/dist/jquery.js"
  },
  "jest": {
    "setupFiles": [
      "./test-env.js"
    ],
    "moduleDirectories": [
      "node_modules",
      "./web/ext/d3"
    ],
    "moduleNameMapper": {
      "^googleTagManager$": "<rootDir>/web/js/components/util/google-tag-manager.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(ol)/)"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/build/"
    ]
  },
  "prettier": {
    "singleQuote": true
  }
}
