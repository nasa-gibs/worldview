{
  "name": "worldview",
  "version": "2.7.0",
  "description": "Interactive interface for browsing full-resolution, global satellite imagery",
  "keywords": [
    "NASA",
    "Earthdata",
    "GIS",
    "NRT",
    "satellite imagery",
    "wildfire management",
    "air quality",
    "flood monitoring",
    "science",
    "earth science",
    "GDAL"
  ],
  "homepage": "https://worldview.earthdata.nasa.gov/",
  "bugs": {
    "url": "https://github.com/nasa-gibs/worldview/issues",
    "email": "support@earthdata.nasa.gov"
  },
  "license": "NASA-1.3",
  "repository": "nasa-gibs/worldview",
  "engines": {
    "node": "8.8.1"
  },
  "scripts": {
    "analyze": "cross-env NODE_ENV=analyze webpack",
    "browserstack": "node e2e/browserstack.js -c e2e/browserstack.conf.js -e browserstack ",
    "build": "run-s clean \"getcapabilities -- {1}\" \"build:config -- {1}\" build:dev --",
    "build:dev": "webpack",
    "build:config": "bash -c 'tasks/buildOptions.sh ${1}' 0 && node ./tasks/config.js",
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:tests": "cross-env NODE_ENV=testing webpack",
    "clean": "node ./tasks/clean.js",
    "dist": "NODE_ENV=production npm run build && node ./tasks/dist.js",
    "dist:dev": "NODE_ENV=development npm run build && node ./tasks/dist.js",
    "distclean": "npm run clean && node ./tasks/distclean.js",
    "e2e": "run-s --continue-on-error --silent e2e:*",
    "e2e:chrome": "nightwatch -e chrome",
    "e2e:firefox": "nightwatch -e firefox --skiptags wip",
    "getcapabilities": "bash -c 'FETCH_GC=1 tasks/buildOptions.sh ${1}' 0",
    "linkcheck": "node ./tasks/linkcheck",
    "lint": "run-s lint:*",
    "lint:css": "stylelint ./web/css/**/*.css",
    "lint:js": "eslint .",
    "preinstall": "node tasks/checkNodeVersion.js",
    "postinstall": "npm run python",
    "python": "virtualenv .python && run-script-os",
    "python:darwin:linux": "PATH=.python/bin:${PATH} pip install -r requirements.txt",
    "python:win32": "bash -c 'PATH=.python/Scripts:${PATH} pip install -r requirements.txt'",
    "report": "node e2e/create-report.js",
    "start": "node ./tasks/start.js",
    "test": "npm run lint && run-p test:server test:run",
    "test:server": "buster-server -c",
    "test:run": "sleep 2 && buster-test && npm run test:end",
    "test:end": "run-script-os",
    "test:end:darwin:linux": "pkill -f run-p",
    "test:end:win32": "tskill node",
    "updateconfig": "run-s getcapabilities build:config",
    "watch": "cross-env NODE_ENV=development webpack-dev-server",
    "upload": "node tasks/upload.js"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.26.3",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.16.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^7.3.0",
    "browserstack-capabilities": "^0.4.0",
    "browserstack-local": "^1.3.0",
    "buster": "github:busterjs/buster#v0.8.0",
    "cheerio": "^1.0.0-rc.2",
    "chromedriver": "2.25.1",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
    "css-hot-loader": "^1.4.1",
    "css-loader": "^1.0.0",
    "cssnano": "^4.0.5",
    "eslint": "^4.13.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "express": "4.15.2",
    "file-loader": "^1.1.11",
    "font-awesome-loader": "^1.0.2",
    "font-awesome-webpack": "git+https://github.com/jarecsni/font-awesome-webpack.git#440af2a2efe7ba1779d039556f04538e57bad4bb",
    "geckodriver": "1.2.0",
    "glob": "^7.1.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "imports-loader": "^0.8.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "moment": "2.13.x",
    "nightwatch": "^0.9.16",
    "node-fetch": "^2.1.2",
    "node-sass": "^4.9.2",
    "node-ssh": "^5.1.1",
    "npm-run-all": "^4.1.1",
    "npm-watch": "^0.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "phantomjs-prebuilt": "2.1.x",
    "postcss-cli": "^4.1.1",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.1.6",
    "postcss-url": "^7.2.1",
    "precss": "^3.1.2",
    "run-script-os": "^1.0.3",
    "sass-loader": "^7.1.0",
    "selenium-server-standalone-jar": "3.7.1",
    "shelljs": "^0.8.2",
    "showdown": "^1.8.6",
    "style-loader": "^0.21.0",
    "stylefmt": "^6.0.0",
    "stylelint": "^8.0.0",
    "tar": "^4.4.4",
    "uglify-js": "^3.4.0",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "underscore": "^1.8.3",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.3",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5",
    "write-file-webpack-plugin": "^4.3.2",
    "yargs": "^11.0.0"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "bootstrap": "^4.1.0",
    "cachai": "^1.0.2",
    "canvg-browser": "^1.0.0",
    "clipboard": "^1.7.1",
    "date-fns": "^1.29.0",
    "file-saver": "^1.3.8",
    "font-awesome": "4.7.0",
    "icheck": "^1.0.2",
    "isotope-layout": "^3.0.6",
    "jquery": "^2.1.4",
    "jquery-bridget": "^2.0.1",
    "jquery-jcrop": "^0.9.13",
    "jquery-ui-bundle": "1.12.1-migrate",
    "jquery-ui-touch-punch": "0.2.3",
    "jquery.joyride": "git+https://git@github.com/nasa-gibs/joyride.git",
    "jscrollpane": "^2.0.22",
    "lodash": "4.15.0",
    "nouislider": "7.0.10",
    "ol": "~4.5.0",
    "perfect-scrollbar": "^0.8.1",
    "proj4": "2.3.3",
    "promise-queue": "2.2.3",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "supercluster": "3.0.2",
    "web-streams-polyfill": "^1.3.2",
    "worldview-components": "^1.9.0",
    "worldview-options-eosdis": "github:nasa-gibs/worldview-options-eosdis#v2.13.0"
  },
  "browser": {
    "d3": "./web/ext/d3/d3.js",
    "gifwriter": "./web/ext/gifwriter/gifwriter.js",
    "neuquant": "./web/ext/neuquant/neuquant.js",
    "mobiscroll": "./web/ext/mobiscroll-2.6.0/mobiscroll.js",
    "isotope-layout": "./node_modules/isotope-layout/dist/isotope.pkgd.js",
    "jquery-jcrop": "./node_modules/jquery-jcrop/js/jquery.Jcrop.js",
    "jquery": "./node_modules/jquery/dist/jquery.js"
  }
}
