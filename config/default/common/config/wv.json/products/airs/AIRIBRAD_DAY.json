{
    "products": {
        "AIRIBRAD_DAY": {
            "name": "AIRS/Aqua Level 1B Infrared (IR) geolocated and calibrated radiances, Day",
            "handler": "MODISSwath",
            "query": {
                "shortName": ["AIRIBRAD", "AIRIBRAD_NRT"],
                "dayNightFlag": "DAY"
            },
            "nrt": {
                "by": "regex",
                "field": "dataset_id",
                "value": "\\(NRT\\)"
            },
            "urs": {
                "by": "constant",
                "value": "true"
            }
        }
    }
}
