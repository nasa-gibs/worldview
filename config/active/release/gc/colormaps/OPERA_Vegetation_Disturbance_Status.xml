      <LegendEntry rgb="0,136,136" tooltip="First Detection &gt;= 50%" id="4"/>
      <LegendEntry rgb="228,135,39" tooltip="Provisional &gt;= 50%" id="5"/>
      <LegendEntry rgb="224,27,7" tooltip="Confirmed &gt;= 50%" id="6"/>
      <LegendEntry rgb="119,119,119" tooltip="Confirmed &lt; 50% finished" id="7"/>
      <LegendEntry rgb="221,221,221" tooltip="Confirmed &gt;= 50% finished" id="8"/>
    </Legend>
  </ColorMap>
</ColorMaps>
