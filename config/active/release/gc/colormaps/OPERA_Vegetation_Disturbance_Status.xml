<?xml version="1.0" encoding="UTF-8"?>
<ColorMaps xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:noNamespaceSchemaLocation="http://gibs.earthdata.nasa.gov/schemas/ColorMap_v1.3.xsd">
  <ColorMap title="Vegation Disturbance Status">
    <Entries>
      <ColorMapEntry rgb="0,0,0" transparent="true" nodata="true" ref="255" />
      <ColorMapEntry rgb="18,18,18" transparent="false" sourceValue="0" ref="0" />
      <ColorMapEntry rgb="0,85,85" transparent="false" sourceValue="1" ref="1"/>
      <ColorMapEntry rgb="137,127,78" transparent="false" sourceValue="2" ref="2"/>
      <ColorMapEntry rgb="222,224,67" transparent="false" sourceValue="3" ref="3" />
      <ColorMapEntry rgb="0,136,136" transparent="false" sourceValue="4" ref="4"/>
      <ColorMapEntry rgb="228,135,39" transparent="false" sourceValue="5" ref="5"/>
      <ColorMapEntry rgb="224,27,7" transparent="false" sourceValue="6" ref="6"/>
